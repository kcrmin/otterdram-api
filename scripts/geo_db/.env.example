# 🌍 Geo DB Upsert 스크립트용 환경 설정 (.env.example)

# === MAIN DB (운영 DB) ===
MAIN_DB_NAME=main_db
MAIN_DB_USER=postgres
MAIN_DB_PASSWORD=your_password
MAIN_DB_HOST=localhost
MAIN_DB_PORT=5432

# === TEMP DB (임시 DB: dump import 용도) ===
TEMP_DB_NAME=temp_db
TEMP_DB_USER=postgres
TEMP_DB_PASSWORD=your_password
TEMP_DB_HOST=localhost
TEMP_DB_PORT=5432

# === SQL dump 파일 절대경로 ===
# 예시: MacOS/Linux 기준 경로
DUMP_FILE=/Users/yourusername/path/to/world.sql

# Windows일 경우
# DUMP_FILE=C:\\Users\\yourusername\\path\\to\\world.sql
